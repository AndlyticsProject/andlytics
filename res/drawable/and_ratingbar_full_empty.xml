<?xml version="1.0" encoding="utf-8"?>
<!--
This is the rating bar drawable that is used to
show a filled star.
-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:drawable="@drawable/rate_star_small_off" android:state_pressed="true" android:state_window_focused="true"/>
    <item android:drawable="@drawable/rate_star_small_off" android:state_focused="true" android:state_window_focused="true"/>
    <item android:drawable="@drawable/rate_star_small_off" android:state_selected="true" android:state_window_focused="true"/>
    <item android:drawable="@drawable/rate_star_small_off"/>

</selector>